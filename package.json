{
  "name": "note-to-frequency",
  "version": "1.2.0",
  "description": "",
  "main": "cjs/index.js",
  "browser": "dist/noteToFrequency.js",
  "module": "es/index.js",
  "jsxnext:main": "es/index.js",
  "files": [
    "dist",
    "cjs",
    "es",
    "src"
  ],
  "scripts": {
    "test": "jest --coverage",
    "prepublish": "npm run lint && npm test && npm run build",
    "lint": "eslint src/*.js",
    "clean": "rimraf es cjs dist",
    "development": "npm run lint && rollup -c -w",
    "build": "npm run clean && npm run build:es && npm run build:cjs && npm run build:umd && npm run build:umd:min",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir cjs",
    "build:umd": "cross-env BABEL_ENV=es rollup -c",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c"
  },
  "keywords": [
    "audio",
    "note",
    "frequency",
    "web audio",
    "music",
    "music theory"
  ],
  "author": "Geoffrey Dhuyvetters <geoffrey.dhuyvetters@gmail.com> (http://landlocked.herokuapp.com/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/duivvv/note-to-frequency.git"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-exponentiation-operator": "^6.8.0",
    "babel-preset-es2015": "^6.16.0",
    "babili": "^0.0.8",
    "cross-env": "^3.1.3",
    "eslint": "^3.7.1",
    "jest": "^16.0.1",
    "rimraf": "^2.5.4",
    "rollup": "^0.36.1",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^2.5.0",
    "eslint-config-devine": "^1.5.0"
  },
  "dependencies": {
    "validate-note": "1.2.3"
  }
