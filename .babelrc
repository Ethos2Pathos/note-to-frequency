{

  "plugins": [
    ["transform-exponentiation-operator"],
    ["transform-flow-strip-types"]
  ],

  "env": {

    "cjs": {
      "presets": [
        ["es2015", {"loose": true}]
      ],
      "plugins": [
        ["add-module-exports"]
      ]
    },

    "es": {
      "presets": [
        ["es2015", {"loose": true, "modules": false}]
      ]
    }

  }

}
